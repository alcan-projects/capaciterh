op, .scroll-bottom\");\r\n    \r\n    scrolls.forEach(scroll =>{\r\n        ScrollTrigger.create({\r\n            trigger: scroll,\r\n            endTrigger:\"body\",\r\n            start: \"top 80%\",\r\n            toggleClass: \"ativo\",\r\n        });\r\n    });\r\n    \r\n    \/\/mudar cor do bg\r\n    \r\n    const bg = document.querySelector(\".bg\")\r\n    \r\n    ScrollTrigger.create({\r\n        trigger: bg,\r\n        start: \"top 60%\",\r\n        end: \"bottom 40%\",\r\n        toggleClass: \"cor\",\r\n    })\r\n        \r\n    \/\/animacao de entrada 2 \r\n    \r\n    const scrolls2 = document.querySelectorAll(\".entrada\");\r\n    \r\n    scrolls2.forEach(scroll2 =>{\r\n        ScrollTrigger.create({\r\n            trigger: scroll2,\r\n            endTrigger:\"body\",\r\n            start: \"top 80%\",\r\n            toggleClass: \"ativo-entrada\",\r\n        });\r\n    });\r\n    \r\n<\/script>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t","protected":false},"excerpt":{"rendered":"<p>Aumente seu potencial de vendas com Landing Pages de Alta Convers\u00e3o! Passe confian\u00e7a, conquiste mais clientes e aumente seu faturamento com Landing Pages de Alta Convers\u00e3o! SOLICITAR OR\u00c7AMENTO DESIGN \u2726 OTIMIZA\u00c7\u00c3O \u2726 RESPONSIVIDADE \u2726 CONVERS\u00c3O \u2726 DESIGN \u2726 OTIMIZA\u00c7\u00c3O \u2726 RESPONSIVIDADE \u2726 CONVERS\u00c3O \u2726 DESIGN \u2726 OTIMIZA\u00c7\u00c3O \u2726 RESPONSIVIDADE \u2726 CONVERS\u00c3O \u2726 DESIGN \u2726 OTIMIZA\u00c7\u00c3O [&hellip;]<\/p>\n","protected":false},"author":1,"featured_media":0,"parent":0,"menu_order":0,"comment_status":"closed","ping_status":"closed","template":"elementor_canvas","meta":{"footnotes":""},"_links":{"self":[{"href":"https:\/\/lukkadesigner.com\/wp-json\/wp\/v2\/pages\/1240"}],"collection":[{"href":"https:\/\/lukkadesigner.com\/wp-json\/wp\/v2\/pages"}],"about":[{"href":"https:\/\/lukkadesigner.com\/wp-json\/wp\/v2\/types\/pag